sed ':a;N;$!ba;s/\n/ /g' res-counteragents.txt > counteragents.json


tr -d '\n' < dl_dict.tmp | sed 's/"/""/g' > t3
echo -n \"[" > before
cat before t3 after >t4
\copy shp.dl_terminals_json from t4 with(format csv);
